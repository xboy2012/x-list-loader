{
  "name": "x-list-loader",
  "version": "1.0.1",
  "description": "A Framework to handle infinite list loading logic in React/Vue/raw javascript",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "keywords": [
    "list",
    "loader"
  ],
  "scripts": {
