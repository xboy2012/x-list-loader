  "bugs": {
    "url": "https://github.com/xboy2012/x-list-loader/issues"
  },
  "homepage": "https://github.com/xboy2012/x-list-loader#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.4.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  }
